# ArcPay SDK Configuration
# Copy this file to .env and fill in your values

# Private key for deployment and testing (with 0x prefix)
PRIVATE_KEY=0x...your_private_key_here

# Circle API Key for Gateway, Bridge, and FX operations
# Get your key from: https://console.circle.com/
CIRCLE_API_KEY=TEST_API_KEY:xxx:yyy

# Arc Testnet RPC URL (default provided)
ARC_TESTNET_RPC=https://rpc.testnet.arc.network

# Optional: Alternative RPC endpoints for fallback
ARC_BACKUP_RPC_1=
ARC_BACKUP_RPC_2=

# Optional: Log level (trace, debug, info, warn, error, fatal)
LOG_LEVEL=info

# Optional: Enable pretty logging in development
NODE_ENV=development
